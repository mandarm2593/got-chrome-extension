{
  "name": "Getting Started Example",
  "version": "1.0",
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab"
  ],
  "description": "Build an Extension!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/got.png",
      "32": "icons/got.png",
      "48": "icons/got.png",
      "128": "icons/got.png"
    },
    "icons": {
      "16": "icons/got.png",
      "32": "icons/got.png",
      "48": "icons/got.png",
      "128": "icons/got.png"
    }
  },
  "options_page": "options.html",
  "manifest_version": 2
}
